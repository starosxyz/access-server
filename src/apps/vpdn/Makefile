MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: PORTAL

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f Makefile.portal static_libs=0 $(@)

.PHONY: PORTAL
PORTAL:
	$(KEEP_GOING)@$(MAKE) -f Makefile.portal all static_libs=0 

project_name_list:
	@echo PORTAL
